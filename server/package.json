{
  "name": "mna-doctrina-server",
  "version": "0.0.1",
  "description": "[MNA] App Template",
  "repository": "https://github.com/mission-apprentissage/doctrina.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "nodemon --ext 'ts,js,cjs,json' --ignore 'tests/' --exec 'ts-node --esm src/index.ts'",
    "build": "tsc -p .",
    "start": "node build/index.js",
    "test": "DOCTRINA_LOG_LEVEL=fatal mocha --exit tests/**/*-test.js",
    "cli": "DOCTRINA_LOG_DESTINATIONS=stderr node src/cli.js",
    "coverage": "nyc --temp-dir .coverage/.nyc_output --report-dir .coverage --reporter=lcov --reporter=html yarn test",
    "lint": "eslint src/ tests/"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.9.1",
    "@sentry/node": "^5.22.3",
    "@sentry/tracing": "^5.22.3",
    "@turf/distance": "^6.0.1",
    "aws-sdk": "^2.677.0",
    "axios": "1.1.3",
    "body-parser": "1.20.0",
    "boom": "7.3.0",
    "bunyan": "1.8.15",
    "bunyan-slack": "0.0.10",
    "burner-email-providers": "1.0.67",
    "changelog-parser": "2.8.0",
    "clamscan": "2.1.2",
    "commander": "9.2.0",
    "convert-csv-to-json": "0.0.15",
    "dotenv": "16.0.0",
    "ejs": "3.1.6",
    "env-var": "7.3.0",
    "express": "4.18.1",
    "express-rate-limit": "5.1.3",
    "form-data": "4.0.0",
    "fs-extra": "9.0.0",
    "joi": "17.6.0",
    "lodash-es": "4.17.21",
    "match-sorter": "6.3.1",
    "miniget": "4.2.1",
    "mjml": "4.10.3",
    "mongodb": "4.5.0",
    "mongoose": "5.10.2",
    "mongoose-paginate": "5.0.3",
    "node-fetch": "3.2.10",
    "nodemailer": "6.6.3",
    "nodemailer-html-to-text": "3.2.0",
    "notion-client": "4.12.0",
    "oleoduc": "0.7.0",
    "pretty-ms": "7.0.1",
    "query-string": "7.1.1",
    "sib-api-v3-sdk": "8.2.1",
    "stream-json": "1.7.4",
    "swagger-ui-express": "4.1.4",
    "tcp-port-used": "1.0.2",
    "xlsx": "0.16.1",
    "yup": "0.32.11"
  },
  "engines": {
    "node": ">=12.11.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@swc/core": "1.3.6",
    "@types/express": "4.17.14",
    "@types/node": "18.8.3",
    "axiosist": "0.10.0",
    "chalk": "4.1.2",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "lint-staged": ">=10",
    "mocha": "10.0.0",
    "mongodb-memory-server": "8.5.2",
    "nock": "13.2.4",
    "nodemon": "2.0.16",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
